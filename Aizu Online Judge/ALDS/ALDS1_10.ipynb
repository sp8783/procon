   "source": [
    "N = int(input())\n",
    "\n",
    "dp = [0]*(N+1)\n",
    "dp[0] = 1\n",
    "dp[1] = 1\n",
    "\n",
    "def solve(n):\n",
    "    if dp[n-2] == 0:\n",
    "        solve(n-1)\n",
    "\n",
    "    if dp[n-1] == 0:\n",
    "        solve(n-1)\n",
    "    \n",
    "    if n >= 2:\n",
    "        dp[n] = dp[n-1] + dp[n-2]\n",
    "    return\n",
    "\n",
    "solve(N)\n",
    "print(dp[N])"
   ]
  }
