   "source": [
    "n = int(input())\n",
    "L = list(map(int,input().split()))\n",
    "\n",
    "from itertools import accumulate\n",
    "l = list(accumulate(L))\n",
    "ll = list(accumulate(l))\n",
    "\n",
    "max1 = L[0]\n",
    "for i in range(n):\n",
    "    if L[i] > max1:\n",
    "        max1 = L[i]\n",
    "    print(ll[i] + (i+1)*max1)"
   ]
  }
