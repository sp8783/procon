    "from collections import Counter\n",
    "N = int(input())\n",
    "r = list(input())\n",
    "\n",
    "R = Counter(r)\n",
    "L = [\"F\", \"D\", \"C\", \"B\", \"A\"]\n",
    "ans = 0\n",
    "\n",
    "for i in range(5):\n",
    "    ans += R[L[i]] * i\n",
    "\n",
    "ans /= N\n",
    "\n",
    "print(ans)"
   ]
  }
