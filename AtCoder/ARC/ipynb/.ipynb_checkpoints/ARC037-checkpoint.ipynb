   "source": [
    "N, K = map(int, input().split())\n",
    "T = [int(input()) for _ in range(N)]\n",
    "\n",
    "tot = T[0] + T[1]\n",
    "ans = -1\n",
    "\n",
    "for i in range(N-2):\n",
    "    tot += T[i+2]\n",
    "    if tot < K:\n",
    "        ans = i+3\n",
    "        break\n",
    "    tot -= T[i]\n",
    "\n",
    "print(ans)"
   ]
