    "import math\n",
    "\n",
    "def lcm(a,b):\n",
    "    lcm = a*b//math.gcd(a,b)\n",
    "    return lcm\n",
    "\n",
    "N = int(input())\n",
    "\n",
    "ans = 1\n",
    "for i in range(2,N+1):\n",
    "    ans = lcm(ans, i)\n",
    "print(ans+1)"
   ]
