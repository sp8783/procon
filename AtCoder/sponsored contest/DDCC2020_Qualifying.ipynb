   "source": [
    "M = int(input())\n",
    "\n",
    "ans = -1\n",
    "S = 0\n",
    "\n",
    "for _ in range(M):\n",
    "    d, c  = map(int, input().split())\n",
    "    S += c*d\n",
    "    ans += c\n",
    "\n",
    "ans += (S-1)//9\n",
    "print(ans)"
   ]
  }
