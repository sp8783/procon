   "source": [
    "K = int(input())\n",
    "\n",
    "if K%9 != 0:\n",
    "    ans = 0\n",
    "else:\n",
    "    dp = [0]*(K+1)\n",
    "    dp[0] = 1\n",
    "    MOD = 10**9+7\n",
    "    for i in range(1,K+1):\n",
    "        b = min(i,9)\n",
    "        for j in range(1, b+1):\n",
    "            dp[i] += dp[i-j]\n",
    "            dp[i] %= MOD\n",
    "    ans = dp[K]\n",
    "\n",
    "print(ans)"
   ]
  }
