    "N, K = map(int, input().split())\n",
    "lst = []\n",
    "for _ in range(N):\n",
    "    a, b = map(int, input().split())\n",
    "    lst.append(b)\n",
    "    lst.append(a-b)\n",
    "lst = sorted(lst)[::-1]\n",
    "ans = sum(lst[:K])\n",
    "print(ans)"
   ]
