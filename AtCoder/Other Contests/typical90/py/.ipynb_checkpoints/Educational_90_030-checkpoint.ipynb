   "source": [
    "N, K = map(int, input().split())\n",
    "\n",
    "c = [0]*(N+10)\n",
    "ans = 0\n",
    "\n",
    "for i in range(2,N+1):\n",
    "    if c[i] != 0:\n",
    "        if c[i] >= K:\n",
    "            ans += 1\n",
    "        continue\n",
    "\n",
    "    if K == 1:\n",
    "        ans += 1\n",
    "    j = 1\n",
    "    while i*j <= N:\n",
    "        c[i*j] += 1\n",
    "        j += 1\n",
    "\n",
    "print(ans)"
   ]
  }
