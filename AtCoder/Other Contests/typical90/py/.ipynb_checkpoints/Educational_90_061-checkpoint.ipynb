   "source": [
    "from collections import deque\n",
    "\n",
    "Q = int(input())\n",
    "q = deque()\n",
    "for _ in range(Q):\n",
    "    t, x = map(int, input().split())\n",
    "    if t == 1:\n",
    "        q.appendleft(x)\n",
    "    elif t == 2:\n",
    "        q.append(x)\n",
    "    else:\n",
    "        print(q[x-1])"
   ]
