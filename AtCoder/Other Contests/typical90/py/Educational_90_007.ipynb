   "source": [
    "from bisect import bisect_left\n",
    "\n",
    "N = int(input())\n",
    "A = list(map(int, input().split()))\n",
    "A = [-1*(10**10)] + sorted(A) + [10**10]\n",
    "\n",
    "Q = int(input())\n",
    "for _ in range(Q):\n",
    "    b = int(input())\n",
    "    i = bisect_left(A, b)\n",
    "    ans = min(A[i]-b, b-A[i-1])\n",
    "    print(ans)"
   ]
