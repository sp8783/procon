   "source": [
    "N = int(input())\n",
    "\n",
    "inf = float(\"inf\")\n",
    "dp = [inf]*(N+1)\n",
    "dp[0] = 0\n",
    "\n",
    "lst = [1, 6, 6**2, 6**3, 6**4, 6**5, 6**6, 9, 9**2, 9**3, 9**4, 9**5]\n",
    "\n",
    "for i in range(1, N+1):\n",
    "    for j in lst:\n",
    "        if i - j >= 0:\n",
    "            dp[i] = min(dp[i], dp[i-j] + 1)\n",
    "\n",
    "ans = dp[N]\n",
    "print(ans)"
   ]
  }
